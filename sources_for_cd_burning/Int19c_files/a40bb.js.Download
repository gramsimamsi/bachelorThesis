$(document).on('click','.pdf-download',function(e){e.preventDefault();var href=$(this).attr('href');$(this).attr('href','#');var _self=$(this);lcntent="";typeofstorage="cookie";if(typeof(Storage)!=="undefined"){console.log('Using local');lcntent=localStorage.getItem("pdfdnld");typeofstorage="local";}
$.ajax({type:'post',url:downloadpdf.ajax_url,data:{'action':'get_download_pdf_item','security':_self.attr('sec'),'pid':_self.attr('pid'),'typeofstorage':typeofstorage,'lcntent':lcntent,},cache:false,success:function(html){console.log(html);console.log($.parseJSON(html));if(html=="true"){window.location=href;}
else{res=$.parseJSON(html);if(res['download']=="true"){if(res['setcntnt']!='malformed'){localStorage.setItem("pdfdnld",JSON.stringify(res['setcntnt']));window.location=href;}}}}});});;$(function(){$(document).on('click','.external-link',function(event){ga('send','event',{'anonymizeIP':true,eventCategory:'Outbound Link',eventAction:'click',eventLabel:event.currentTarget.href});});});